{
  "extends": ["airbnb", "prettier"],
  "plugins": ["compat", "prettier"],
  "env": {
    "es6": true,
    "node": true,
    "browser": true,
    "jest": true
  },
  "globals": {
    "google": true
  },
  "rules": {
    "semi": [2, "always"],
    "linebreak-style": 0,
    "compat/compat": 2,
    "no-param-reassign": 0,
    "jsx-a11y/interactive-supports-focus": 0,
    "import/prefer-default-export": 1,
    "no-underscore-dangle": 1,
    "comma-dangle": [
      "error",
      {
        "arrays": "never",
        "objects": "always",
        "imports": "never",
        "exports": "never",
        "functions": "never"
      }
    ]
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  }
}
